                                                                                                                      owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                                                                                             owl:onDatatype xsd:int ;
                                                                                                                                             owl:withRestrictions ( [ xsd:minInclusive "17"^^xsd:int
                                                                                                                                                                    ]
                                                                                                                                                                  )
