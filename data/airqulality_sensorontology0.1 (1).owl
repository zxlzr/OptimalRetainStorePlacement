###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Emergent

:Emergent rdf:type owl:Class ;
          
          rdfs:label "严重污染" ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:intersectionOf ( :Air
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasAQIValue ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                                         owl:onDatatype xsd:int ;
                                                                         owl:withRestrictions ( [ xsd:minExclusive "300"^^xsd:int
                                                                                                ]
                                                                                              )
                                                                       ]
                                                     ]
                                                   )
                              ] ;
          
          rdfs:comment "危险" .



###  http://smog.locigraph.cn/ontologies/2016/6/sensor#EveningRushHour

:EveningRushHour rdf:type owl:Class ;
                 
                 rdfs:label "晚高峰时段" ;
                 
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:intersectionOf ( :HourOfDay
