###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Wind

:Wind rdf:type owl:Class ;
      
      rdfs:label "风" ;
      
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasWindBeaufortScaleValue ;
                        owl:someValuesFrom xsd:int
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasWindDirectionValue ;
                        owl:someValuesFrom xsd:int
                      ] .



###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Windy

:Windy rdf:type owl:Class ;
       
       rdfs:label "有风" ;
       
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:intersectionOf ( :WeatherType
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasWeatherDescriptionValue ;
                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                         owl:oneOf [ rdf:type rdf:List ;
                                                                                     rdf:first "Windy" ;
                                                                                     rdf:rest rdf:nil
                                                                                   ]
                                                                       ]
                                                  ]
                                                )
                           ] .



