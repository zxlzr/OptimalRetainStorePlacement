                                                                                                 owl:withRestrictions ( [ xsd:minInclusive "10"^^xsd:int
                                                                                                                        ]
                                                                                                                      )
                                                                                               ]
                                                                                               [ rdf:type rdfs:Datatype ;
                                                                                                 owl:onDatatype xsd:int ;
