         rdfs:label "休息日" ;
         
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:intersectionOf ( :DayOfWeek
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasDayValue ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                                        owl:unionOf ( [ rdf:type rdfs:Datatype ;
                                                                                        owl:onDatatype xsd:int ;
                                                                                        owl:withRestrictions ( [ xsd:minInclusive "7"^^xsd:int
                                                                                                               ]
                                                                                                             )
                                                                                      ]
                                                                                      [ rdf:type rdfs:Datatype ;
                                                                                        owl:onDatatype xsd:int ;
                                                                                        owl:withRestrictions ( [ xsd:maxInclusive "1"^^xsd:int
                                                                                                               ]
                                                                                                             )
                                                                                      ]
                                                                                    )
                                                                      ]
                                                    ]
                                                  )
                             ] .



