###  http://smog.locigraph.cn/ontologies/2016/6/sensor#HourOfDay

:HourOfDay rdf:type owl:Class ;
           
           rdfs:label "一天中的某小时" ;
           
           rdfs:subClassOf :Time ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasHourValue ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange [ rdf:type rdfs:Datatype ;
                                               owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                      owl:onDatatype xsd:int ;
                                                                      owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:int
                                                                                             ]
                                                                                           )
                                                                    ]
                                                                    [ rdf:type rdfs:Datatype ;
                                                                      owl:onDatatype xsd:int ;
                                                                      owl:withRestrictions ( [ xsd:maxExclusive "24"^^xsd:int
