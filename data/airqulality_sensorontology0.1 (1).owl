#################################################################
#
#    Classes
#
#################################################################


###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Air

:Air rdf:type owl:Class ;
     
     rdfs:label "空气质量" ;
     
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasAQIValue ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onDataRange xsd:int
                     ] .



###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Beijing_Station

:Beijing_Station rdf:type owl:Class ;
                 
                 rdfs:label "北京观测站" ;
                 
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :Dongsi
                                                   :Shunyixincheng
                                                   :Changping
                                                   :Dingling
                                                   :Guanyuan
                                                   :Gucheng
                                                   :Nongzhanguan
                                                   :Wanshouxigong
                                                   :Aotizhongxin
                                                   :Tiantan
                                                   :Haidianquwanliu
                                                   :Huairou
                                                 )
                                     ] ;
                 
                 rdfs:subClassOf :Sensor_Location .



###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Breezy

:Breezy rdf:type owl:Class ;
        
        rdfs:label "有微风" ;
        
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:intersectionOf ( :WeatherType
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasWeatherDescriptionValue ;
                                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                          owl:oneOf [ rdf:type rdf:List ;
                                                                                      rdf:first "Breezy" ;
                                                                                      rdf:rest rdf:nil
                                                                                    ]
                                                                        ]
                                                   ]
                                                 )
                            ] .



###  http://smog.locigraph.cn/ontologies/2016/6/sensor#Calm

:Calm rdf:type owl:Class ;
      
      rdfs:label "无风" ;
      
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:intersectionOf ( :Wind
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasWindBeaufortScaleValue ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                                     owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                                            owl:onDatatype xsd:int ;
                                                                                            owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:int
                                                                                                                   ]
                                                                                                                 )
                                                                                          ]
                                                                                          [ rdf:type rdfs:Datatype ;
                                                                                            owl:onDatatype xsd:int ;
                                                                                            owl:withRestrictions ( [ xsd:maxInclusive "0"^^xsd:int
                                                                                                                   ]
                                                                                                                 )
                                                                                          ]
                                                                                        )
                                                                   ]
                                                 ]
                                               )
                          ] ;
      
      rdfs:subClassOf :Wind .



